{
  "name": "trpc-notes-api",
  "version": "1.0.0",
  "description": "A Notes API built with tRPC, Prisma, and Zod",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx src/seed.ts",
    "test:client": "tsx src/test-client.ts"
  },
  "keywords": ["trpc", "prisma", "zod", "notes", "api", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@trpc/server": "^10.45.0",
    "@trpc/client": "^10.45.0",
    "zod": "^3.22.4",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.2",
    "tsx": "^4.6.2"
  }
} 